<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff3cb371"
    tools:context=".Score">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="462dp"
        android:layout_gravity="left|top"
        android:weightSum="1"
        android:background="#ff000000">


        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="BUILDINGS"
            android:id="@+id/textView3"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ffff6600"
            android:gravity="center_vertical|center_horizontal"/>

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="COURSES"
            android:id="@+id/textView4"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#cccc6600"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="DORMS"
            android:id="@+id/textView5"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#cccc668c"
            android:gravity="center_vertical|center_horizontal"
            android:visibility="visible"
            android:autoText="false" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="TRAFFIC"
            android:id="@+id/textView6"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff9966ff"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="MEAL PLACES"
            android:id="@+id/textView7"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff9999cc"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="BUSES"
            android:id="@+id/textView8"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff6699cc"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="INSTRUCTORS"
            android:id="@+id/textView9"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff3399ff"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="STAFF"
            android:id="@+id/textView10"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff33cc99"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="SHOPPING"
            android:id="@+id/textView11"
            android:layout_gravity="center_horizontal|left"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff33cc66"
            android:gravity="center_vertical|center_horizontal" />

        <SeekBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar" />

    </LinearLayout>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Main"
        android:id="@+id/button18"
        android:layout_gravity="left|bottom"
        android:textSize="30sp"
        android:textColor="#ffff001e"
        android:textStyle="bold|italic"
        android:background="#ff3cb3ff"
        android:clickable="true"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="RESET"
        android:id="@+id/button19"
        android:layout_gravity="right|bottom"
        android:textSize="30sp"
        android:textColor="#ffff001e"
        android:textStyle="bold|italic"
        android:background="#ff3cb3ff"
        android:clickable="true"/>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="90dp"
        android:layout_height="fill_parent"
        android:layout_gravity="right|top"
        android:weightSum="1">

        <TextView
            android:layout_width="85dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView12"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ffff6600"
            android:gravity="center_vertical|center_horizontal"
             />

        <TextView
            android:layout_width="85dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView13"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"

            android:background="#cccc6600"
            android:gravity="center_vertical|center_horizontal"/>

        <TextView
            android:layout_width="85dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView14"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#cccc668c"
            android:gravity="center_vertical|center_horizontal"
            android:visibility="visible" />

        <TextView
            android:layout_width="85dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView15"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff9966ff"
            android:gravity="center_vertical|center_horizontal"/>

        <TextView
            android:layout_width="85dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView16"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff9999cc"
            android:gravity="center_vertical|center_horizontal"/>

        <TextView
            android:layout_width="86dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView17"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff6699cc"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="85dp"
            android:layout_height="52dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView18"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff3399ff"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="85dp"
            android:layout_height="50dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView19"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff33cc99"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="85dp"
            android:layout_height="50dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="0"
            android:id="@+id/textView20"
            android:layout_gravity="center_horizontal|right"
            android:textSize="25sp"
            android:textColor="#ffff001e"
            android:textStyle="bold|italic"
            android:background="#ff33cc66"
            android:gravity="center_vertical|center_horizontal" />
    </LinearLayout>
    />


    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->

</FrameLayout>
